{
  "componentPath": "./src/wwObject.vue",
  "editor": {
    "label": {
      "en": "Slider"
    },
    "settingsOptions": {
      "slides": {
        "path": "slides",
        "label": { "en": "Slides", "fr": "Slides" },
        "type": "Tabs",
        "itemsLabel": "Slide"
      },
      "slidesPerView": {
        "type": "Number",
        "label": {
          "en": "Slides per view",
          "fr": "Slides per view"
        },
        "options": {
          "min": 1,
          "max": 6,
          "step": 1
        }
      },
      "spaceBetween": {
        "type": "Length",
        "label": {
          "en": "Space between",
          "fr": "Space between"
        },
        "options": {
          "unitChoices": [
            { "value": "px", "label": "px", "min": 1, "max": 1000 }
          ]
        }
      },
      "direction": {
        "label": {
          "en": "Direction",
          "fr": "Direction"
        },
        "type": "TextSelect",
        "options": {
          "options": [
            {
              "value": "horizontal",
              "label": { "en": "horizontal" }
            },
            { "value": "vertical", "label": { "en": "vertical" } }
          ]
        }
      },
      "effect": {
        "label": {
          "en": "Effect",
          "fr": "Effect"
        },
        "type": "TextSelect",
        "options": {
          "options": [
            { "value": "slide", "label": { "en": "slide" } },
            { "value": "fade", "label": { "en": "fade" } }
          ]
        }
      },
      "navigation": {
        "type": "OnOff",
        "label": {
          "en": "Navigation",
          "fr": "Navigation"
        }
      },
      "pagination": {
        "type": "OnOff",
        "label": {
          "en": "Pagination",
          "fr": "Pagination"
        }
      },
      "loop": {
        "type": "OnOff",
        "label": {
          "en": "Loop",
          "fr": "Loop"
        }
      }
    }
  }
}
